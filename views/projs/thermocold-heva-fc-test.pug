      body { padding-top: 20px; padding-bottom: 50px; }
      blockquote {
      	font-size: 13px;
      	padding: 5px 10px;
      }
      .well { margin-bottom: 20px; box-shadow: 0px 15px 20px 0px rgba(0, 0, 0, 0.25); }
      .well-sm {
        padding: 10px;
        border-radius: 3px;
      }
      .panel { box-shadow: 0px 15px 20px 0px rgba(0, 0, 0, 0.25); }
