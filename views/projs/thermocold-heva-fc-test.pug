    script.
      $(function(){
      	let getUserName = () => {
      		let userName = localStorage.getItem('userName') || "Undefined";
      		return userName;
      	};
      	
      	// v1
      	//$.notify.defaults( {autoHideDelay: 10000} );
      	// v3
      	//alertify.custom = alertify.extend("custom");
      	// v4
      	if(!alertify.myAlert){
      		//define a new dialog
      		alertify.dialog(
            'myAlert',
            function(){
        			return{
        					main:function(message){
        						this.message = message;
        					},
        					setup:function(){
        						return { 
        							buttons:[{text: "cool!", key:27/*Esc*/}],
        							focus: { element:0 }
        						};
        					},
        					prepare:function(){
        						this.setContent(this.message);
        					}
        			}
      		  }
          );
      	};
        
        function _getOutput(m){
          let model = m, //document.getElementById("model").value,
            Taoutdoor = document.getElementById("Taoutdoor").value,
            Twin = document.getElementById("Twin").value,
            Twout = document.getElementById("Twout").value,
            egp = document.getElementById("egp").value,
            Taoutdoor_FC = document.getElementById("Taoutdoor_FC").value,
            Twin_FC = document.getElementById("Twin_FC").value,
            Twout_FC = document.getElementById("Twout_FC").value,
            egp_FC = document.getElementById("egp_FC").value;
          let delta = parseFloat(document.getElementById("delta").value),
      			delta_FC = parseFloat(document.getElementById("delta_FC").value);
          let stuff;
        	if(egp > 0.0){ stuff = "ethylene_glycol" }else{stuff = "water"};
          let output = interpolateAndConvert.thermocold_heva_fc(model, Taoutdoor, Twin, Twout, stuff, egp, delta);
          //let Pf = output.Pf.value.toFixed(2);
          return output;
        }
        
      	function calc_test(){
      		$("#report").html('<p>Was not calculated...</p>');
      		$("#result").html('<p>Was not calculated...</p>');
      		
      		$("#Twin").val(parseFloat($("#Twout").val()) + parseFloat($("#delta").val()));
      		$("#Twin_FC").val(parseFloat($("#Twout_FC").val()) + parseFloat($("#delta_FC").val()));

      		let model = document.getElementById("model").value,
      			Taoutdoor = document.getElementById("Taoutdoor").value,
      			Twin = document.getElementById("Twin").value,
      			Twout = document.getElementById("Twout").value,
      			egp = document.getElementById("egp").value,
      			Taoutdoor_FC = document.getElementById("Taoutdoor_FC").value,
      			Twin_FC = document.getElementById("Twin_FC").value,
      			Twout_FC = document.getElementById("Twout_FC").value,
      			egp_FC = document.getElementById("egp_FC").value;
      		
      		if( model==='' || Taoutdoor==='' || Twin==='' || Twout==='' || egp===''){
      			// v1: Notify.js
      			//$.notify("Abobted. Please, check the input form", "warn");
      			// v2: pnotify
      			/*
      			new PNotify({
      				title: 'Warning!',
      				text: 'Abobted. Please, check the input form',
      				addclass: 'custom',
      				icon: 'fa fa-file-image-o',
      				nonblock: {
      					nonblock: true
      				},
      				type: 'warning'
      			});*/
      			// v3
      			//alertify.error("Could not be calculated. Please, check the input form", 10000);
      			// v4
      			//alertify.notify( message, 'success', [wait, callback]);
      			alertify.notify( `Hey, ${getUserName()}! Please, check the input form`, 'error', 10);
      			return;
      		}

      		let delta = parseFloat(document.getElementById("delta").value),
      			delta_FC = parseFloat(document.getElementById("delta_FC").value);
      			
      		//console.log(Taoutdoor_FC, Twin_FC, Twout_FC, egp_FC, delta_FC)

      		let stuff, stuff_FC;
      		if(egp > 0.0){ stuff = "ethylene_glycol" }else{stuff = "water"};
      		if(egp_FC > 0.0){ stuff_FC = "ethylene_glycol" }else{stuff_FC = "water"};
      		let output = interpolateAndConvert.thermocold_heva_fc(model, Taoutdoor, Twin, Twout, stuff, egp, delta);
      		
      		let output_FC = interpolateAndConvert.thermocold_heva_fc(model, Taoutdoor, Twin_FC, Twout_FC, stuff_FC, egp_FC, delta_FC);
      		
      		// --- FREECOOLING output data
      		let coolingCapacityCoefficient = interpolateAndConvert.freeCoolingMode(
      			"HEVA FC",
      			Taoutdoor_FC,
      			Twin_FC
      		).coolingCapacityCoefficient;
      		let FreeCoolingQ = output_FC.Pf.value * coolingCapacityCoefficient;
      		console.log(FreeCoolingQ)
      		// --- ---
      		
      		$("#report").html(
      			"<blockquote>"+
      			"inCatalogTableRange = "+output.report.inCatalogTableRange+"<br />"+
      			"inCatalogDiagrammRange = "+output.report.inCatalogDiagrammRange+
      			"<br />"+
      			"<strong>ETH. GLYCOL</strong><br />"+
      			"freezingTemperature = "+output.report.ethyleneGlycol.freezingTemperature.toFixed(2)+"<br />"+
      			"safetySuggestedLimit = "+output.report.ethyleneGlycol.safetySuggestedLimit.toFixed(2)+"<br />"+
      			"coolingCapacityCoefficient = "+output.report.ethyleneGlycol.coolingCapacityCoefficient.toFixed(2)+"<br />"+
      			"inputCapacityCoefficient = "+output.report.ethyleneGlycol.inputCapacityCoefficient.toFixed(2)+"<br />"+
      			"flowCoefficient = "+output.report.ethyleneGlycol.flowCoefficient.toFixed(2)+"<br />"+
      			"lossPressureCoefficient = "+output.report.ethyleneGlycol.lossPressureCoefficient.toFixed(2)+
      			"<br/>"+
      			//"<strong>FREE COOLING MODE</strong><br />"+
      			//"lossPressureCoefficient = "+output.report.ethyleneGlycol.lossPressureCoefficient.toFixed(2)+
      			//"<br />"+
      			"<strong>FOR DELTA DIFFERENT TO NOMINAL</strong><br />"+
      			"coolingCapacityFactors = "+output.report.forDeltaDifferentToNominal.coolingCapacityFactors.toFixed(2)+"<br />"+
      			"inputPowerCorrectionFactors = "+output.report.forDeltaDifferentToNominal.inputPowerCorrectionFactors.toFixed(2)+
      			"</blockquote>"
      		);
      		$("#result").html(
      			"<blockquote>"+
            ($('#Q').val()==0?"<strong>Q was not set</strong><br />":"<strong>Q was required as " + $('#Q').val() + " kW</strong><br />")+
      			"<strong>FREECOOLING OFF</strong><br />"+
      			"Pf = "+output.Pf.value.toFixed(2)+" kW<br />"+
      			"Pa = "+output.Pa.value.toFixed(2)+" kW<br />"+
      			"qw = "+output.qw.value.toFixed(2)+" l/sec = "+(output.qw.value*3.6).toFixed(2)+" m3/h<br />"+
      			"dpw = "+output.dpw.value.toFixed(2)+" kPa"+
      			"<br />"+
      			"<strong>FREECOOLING ON</strong><br />"+
      			//"Under construction...<br />"+
      			"coolingCapacityCoefficient = "+coolingCapacityCoefficient.toFixed(2)+"<br />"+
      			"<span class='text-danger'>Pfree = "+FreeCoolingQ.toFixed(2)+" kW (should be checked because it could be incorrect)</span><br />"+
      			"</blockquote>"
      		);
      		// v1
      		//$.notify("Check the result please", "success");
      		// v2: pnotify
      		/*
      		new PNotify({
      			title: 'Success!',
      			text: 'Check the result please',
      			type: 'success'
      		});*/
      		// v3: alertify
      		//alertify.success("Ok. Check the result please", 10000);
      		// v4
      		alertify.notify( 'Ok. Check the result please', 'success', 5);
      	}//end of calc_test
      	
      	$('#calc').click(function(e){
      		e.preventDefault();
      		calc_test();
      	});
      	let hideLies = () => {
      		let input = document.getElementById('searchModel'),
      			filter = input.value.toUpperCase(),
      			ul = document.getElementById('myUL'),
      			li = ul.getElementsByTagName('li');
      			
      		// Loop through all list items, and hide those who don't match the search query
      		for (i = 0; i < li.length; i++) {
      			a = li[i].getElementsByTagName("a")[0];
            
            /*
                                    HEEY!
            */
            
      			if(                  // TODO: Additional check by Q should de here!
              a.innerHTML.toUpperCase().indexOf(filter) > -1 &&
              _getOutput(a.innerHTML).Pf.value >= _getNumericValue($('#Q').val())
            ){
      				li[i].style.display = "";
      			} else {
      				li[i].style.display = "none";
      			}
      		}
      	};
      	//$("#model").change(function(){calc_test();});
      	$('#myUL').find('li > a').click(function(e){
      		e.preventDefault();
      		
      		$("#searchModel").val(this.text);
      		$("#model").val(this.text);
      		
      		// Need to filter
      		hideLies();
      	});
      	$("#searchModel").keyup(function(){
      		
      		// Need to filter
      		hideLies();
      		
      		$("#model").val('');
      	});
      	
      	$("#Taoutdoor").keyup(function(){calc_test();});
      	$("#Twin").keyup(function(){calc_test();});
      	$("#Twout").keyup(function(){
      		$("#Twin").val(
      			Number($("#Twout").val())+Number($("#delta").val())
      		)
      		calc_test();
      	});
      	$("#egp").keyup(function(){calc_test();});
      	$("#delta").keyup(function(){calc_test();});
      	$("#Taoutdoor_FC").keyup(function(){calc_test();});
      	$("#Twin_FC").keyup(function(){calc_test();});
      	$("#Twout_FC").keyup(function(){calc_test();});
      	$("#egp_FC").keyup(function(){calc_test();});
      	$("#delta_FC").keyup(function(){calc_test();});

        let _setDefault = (totalDefault) => {
          $("#report").html('<p>Was not calculated...</p>');
          $("#result").html('<p>Was not calculated...</p>');
          
          $("#searchModel").val("");
          $("#model").val("");
          
          if(totalDefault===true){
            $("#Q").val(0);
            
            $("#Taoutdoor").val(35.0),
            $("#Twout").val(10.0);
            $("#egp").val(30.0);
            $("#delta").val(5);
            $("#Twin").val(Number($("#Twout").val())+Number($("#delta").val()));
            // FC ON
            $("#Taoutdoor_FC").val(-1.2),
            $("#Twout_FC").val(10.0);
            $("#egp_FC").val(30.0);
            $("#delta_FC").val(5);
            $("#Twin_FC").val(Number($("#Twout_FC").val())+Number($("#delta_FC").val()));
          }
          hideLies();
          
          //$.notify("Default settings was set", "success");
          // v2: pnotify
          /*
          new PNotify({
            title: 'Success!',
            text: `${totalDefault===true?'Default settings was set':'Conditions was not updated'}`,
            type: 'success'
          });
          */
          // v3
          //alertify.log("Default settings was set", 10000);
          // v4
          alertify.notify( `${totalDefault===true?'Default settings was set':'Conditions was not updated'}`, 5);
        };
      	$("#set_default").click(function(e){
      		e.preventDefault();
      		_setDefault(false);
      	});
        $("#setQFromLocalStorage").click(function(e){
          e.preventDefault();
          $("#Q").val(getQFromLocalStorage().toFixed(2));
          
          $('#searchModel').val('');
          $("#model").val("");
          hideLies();
          
          $("#report").html('<p>Was not calculated...</p>');
          $("#result").html('<p>Was not calculated...</p>');
        });
        $('#Q').keyup(function(e){
          $('#searchModel').val('');
          $("#model").val("");
          hideLies();
          
          $("#report").html('<p>Was not calculated...</p>');
      		$("#result").html('<p>Was not calculated...</p>');
        });
      	
      	/*
      	$("#pdf").click(function(){
      		var pdf = function(){
      			console.log('hello world');
      			// Default export is a4 paper, portrait, using milimeters for units
      			var doc = new jsPDF()

      			doc.text('Hello world!', 10, 10);
      			doc.save('a4.pdf')
      		}();
      		
      	})*/
      	
        function _getNumericValue(val){ return (val!=="" && !isNaN(val)) ? Number(val) : "" }
        function getQFromLocalStorage(){
          let coolingCapacity = _getNumericValue(localStorage.getItem('coolingCapacity')) || 0;
      		return coolingCapacity;
        }
        
      	alertify.prompt("Please, confirm your user name", getUserName(),
      		function(evt, value){
      			//console.log(evt);
      			alertify.notify(`Ok, ${value}. I got it...`, 'success', 5);
      			localStorage.setItem('userName', value);
      			
      			//$("#set_default").click();
            _setDefault(true);
      		},
      		function(){
      			alertify.notify(`Ok. So you are ${getUserName()} same as the last time`, 'warning', 5);
      			
      			//$("#set_default").click();
            _setDefault(true);
      		}
      	);
      }());
